{
  "name": "braintree-dropin-react",
  "version": "0.0.4",
  "description": "React Drop-in Payment UI Component",
  "main": "index.js",
  "scripts": {
    "prepare": "npm run build",
    "lint": "./node_modules/eslint/bin/eslint.js ./src --fix",
    "test": "./node_modules/jest/bin/jest.js",
    "update-test": "npm run test -- -u",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && ./node_modules/babel-cli/bin/babel.js ./src/index.js --out-file ./dist/index.js",
    "example": "./node_modules/webpack/bin/webpack.js && ./node_modules/webpack-dev-server/bin/webpack-dev-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kovndr/braintree-dropin-react.git"
  },
  "keywords": [
    "braintree",
    "payment",
    "drop-in v3",
    "react",
    "react-component"
  ],
  "author": "Nandor Kovacs <kovndr@gmail.com> (http://www.kovndr.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kovndr/braintree-dropin-react/issues"
  },
  "homepage": "https://github.com/kovndr/braintree-dropin-react#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "braintree-web-drop-in": "^1.3.1",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-babel": "^6.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^20.0.1",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  }
}
